            mostEvents
        
        -->

        <!-- 

            TODO sort all pages based on distinct weights

        -->
        
    </article>

    <hr class="main-hr">

    <article aria-labelledby="axeSummary">

        <h2 id="axeSummary" tabindex="-1">axeSummary of latest audit ({{dateTimeLatest}})</h2>

        {{> audit_details axeSummary}}
       
        <h3 id="axeDetails" tabindex="-1">axe details</h3>
        <div id="js_axe" class="js_generate_details" data-title="All {{distinctUrlsCount}} URLs with aXe audit"  style="min-height:{{heightPerUrl distinctUrlsCount 80}}">Please wait, loading...</div>

    </article>

    <hr class="main-hr">

    <article aria-labelledby="siteimproveSummary">
        <h2 id="siteimproveSummary" tabindex="-1">siteimproveSummary  of latest audit ({{dateTimeLatest}})</h2>

        {{> audit_details siSummary}}

        <h3 id="siteimproveDetails" tabindex="-1">siteimprove details</h3>
        <div id="js_si" class="js_generate_details" data-title="All {{distinctUrlsCount}} URLs with Siteimprove audit"  style="min-height:{{heightPerUrl distinctUrlsCount 80}}">Please wait, loading...</div>

    </article>

    <hr class="main-hr">

    <article aria-labelledby="lighthouseSummary">
        <h2 id="lighthouseSummary" tabindex="-1">lighthouseSummary  of latest audit ({{dateTimeLatest}})</h2>

        {{> lighthouse_details lhSummary}}

        <h3 id="lighthouseDetails" tabindex="-1">lighthouse details</h3>
        <div id="js_lh" class="js_generate_details" data-title="All {{distinctUrlsCount}} URLs with Lighthouse audit" style="min-height:{{heightPerUrl distinctUrlsCount 80}}">Please wait, loading...</div>

    </article>

    <hr class="main-hr">

    <article aria-labelledby="complexitySummary">
        <h2 id="complexitySummary" tabindex="-1">complexitySummary  of latest audit ({{dateTimeLatest}})</h2>

        {{> complexity_details cpxSummary}}

        <h3 id="complexityDetails" tabindex="-1">Complexity details</h3>
        <div id="js_cpx" class="js_generate_details" data-title="All {{distinctUrlsCount}} URLs with complexity audit" style="min-height:{{heightPerUrl distinctUrlsCount 80}}">Please wait, loading...</div>

    </article>

    <hr class="main-hr">

    <nav aria-label="Bottom navigation">
        <ul class="strong-navi-list">
            <li>
                <a href="#top">Back to top of page</a>
            </li>
            <li>
                <a href="#top-navigation">Back to navigation</a>
